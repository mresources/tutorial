          RowBox[{"MatchQ", "[", 
           RowBox[{"f", ",", 
            RowBox[{"{", "}"}]}], "]"}], ",", "\n", "\t\t\t\t", 
          RowBox[{"Replace", "[", "\n", "\t\t\t\t\t", 
           RowBox[{
            RowBox[{"ReadLine", "@", "textStream"}], ",", 
            RowBox[{"{", "\n", "\t\t\t\t\t\t", 
             RowBox[{
              RowBox[{"EndOfFile", ":>", 
               RowBox[{"Return", "[", "]"}]}], ",", "\n", "\t\t\t\t\t\t", 
              RowBox[{"s_String", "\[RuleDelayed]", "\n", "\t\t\t\t\t\t", 
               RowBox[{"StringReplace", "[", 
                RowBox[{"s", ",", "\n", "\t\t\t\t\t\t\t", 
                 RowBox[{
                  RowBox[{"key__", "~~", "\"\<  \>\"", "~~", "desc__", "~~", 
                   RowBox[{"(", 
                    RowBox[{"\"\<  \>\"", "|", "EndOfString"}], ")"}]}], 
                  "\[RuleDelayed]", 
                  RowBox[{"Sow", "@", 
