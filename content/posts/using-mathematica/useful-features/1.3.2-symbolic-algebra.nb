Cell["\<\
We could even write a function that will solve the equation for an arbitrary \
value and plot the result:\
\>", "Text",
 CellChangeTimes->{
  3.685311620629414*^9, {3.686359411262411*^9, 
   3.6863594727539062`*^9}},ExpressionUUID->"ed9984e5-8876-42cf-bed2-\
a589205c6ec0"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"solveAndPlot", "[", 
    RowBox[{"eqInX_", ",", "val_"}], "]"}], ":=", "\n", "\t", 
   RowBox[{"Show", "[", "\n", "\t\t", 
    RowBox[{
     RowBox[{"Plot", "[", 
      RowBox[{"eqInX", ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"-", "2"}], ",", "2"}], "}"}]}], "]"}], ",", "\n", "\t", 
     "\t", 
     RowBox[{"ListPlot", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "val"}], "}"}], ",", "\n", "\t", "\t\t", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"x", "/.", 
           RowBox[{"Solve", "[", 
            RowBox[{
             RowBox[{"eqInX", "\[Equal]", 
              RowBox[{"(", 
               RowBox[{"1.", "val"}], ")"}]}], ",", "x", ",", "Reals"}], 
            "]"}]}]}], "}"}]}], "]"}], "]"}], ",", "\n", "\t\t", 
     RowBox[{"PlotLabel", "\[Rule]", "\n", "\t\t\t", 
      RowBox[{"Row", "@", 
       RowBox[{"{", "\n", "\t\t\t\t", 
        RowBox[{
         RowBox[{"HoldForm", "[", 
          RowBox[{"eqInX", "\[Equal]", "val"}], "]"}], ",", "\"\< @ \>\"", 
         ",", "\n", "\t", "\t", "\t\t", 
         RowBox[{"Row", "@", 
          RowBox[{"Riffle", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"x", "/.", 
              RowBox[{"Solve", "[", 
               RowBox[{
                RowBox[{"eqInX", "\[Equal]", 
                 RowBox[{"(", 
                  RowBox[{"1.", "val"}], ")"}]}], ",", "x", ",", "Reals"}], 
               "]"}]}], ")"}], ",", "\"\<, \>\""}], "]"}]}]}], "\n", "\t", 
        "\t", "\t\t", "}"}]}]}]}], "\n", "\t\t", "]"}]}], ";"}]], "Code",
 GeneratedCell->False,
 CellChangeTimes->{{3.686359476628025*^9, 3.686359638174994*^9}, {
  3.740950114640583*^9, 
